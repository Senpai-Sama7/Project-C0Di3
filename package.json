{
  "name": "core-agent",
  "version": "1.0.0",
  "description": "Natural Language Cybersecurity Assistant with AI-powered security tools and learning capabilities",
  "main": "bin/cli.js",
  "bin": {
    "core": "./bin/core",
    "core-agent": "./bin/cli.js"
  },
  "scripts": {
    "start": "node bin/cli.js",
    "core": "node bin/core",
    "dev": "ts-node bin/cli.js",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md,yml,yaml}\"",
    "typecheck": "tsc --noEmit",
    "deploy": "bash scripts/deploy-production.sh",
    "test-production": "bash scripts/test-production.sh",
    "build-llama": "bash scripts/build-llama-production.sh",
    "prepare": "npm run build"
  },
  "keywords": [
    "cybersecurity",
    "ai",
    "security",
    "penetration-testing",
    "red-team",
    "blue-team",
    "natural-language",
    "llm",
    "local-ai",
    "privacy"
  ],
  "author": "Core Agent Team",
  "license": "MIT",
  "dependencies": {
    "@jest/expect-utils": "^29.5.0",
    "@types/node": "^20.8.0",
    "@types/prompts": "^2.0.14",
    "axios": "^1.10.0",
    "chalk": "^5.3.0",
    "clipboardy": "^3.0.0",
    "commander": "^11.0.0",
    "cosmiconfig": "^9.0.0",
    "dotenv": "^16.3.1",
    "execa": "^7.1.1",
    "expect": "^29.5.0",
    "fast-glob": "^3.3.1",
    "jest-diff": "^29.5.0",
    "jest-matcher-utils": "^29.5.0",
    "jsonwebtoken": "^9.0.2",
    "ora": "^7.0.1",
    "p-map": "^5.5.0",
    "pretty-format": "^29.5.0",
    "prompts": "^2.4.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "uuid": "^11.1.0",
    "zod": "^3.22.4",
    "zx": "^7.2.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/core-agent.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/core-agent/issues"
  },
  "homepage": "https://github.com/your-org/core-agent#readme"
}
