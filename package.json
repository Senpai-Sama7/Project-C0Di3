{
  "name": "core-agent",
  "version": "1.0.0",
  "description": "Natural Language Cybersecurity Assistant with AI-powered security tools and learning capabilities",
  "main": "dist/bin/cli.js",
  "bin": {
    "core": "./dist/bin/core",
    "core-agent": "./dist/bin/cli.js"
  },
  "scripts": {
    "clean": "rm -rf dist coverage .eslintcache",
    "prebuild": "npm run clean",
    "build": "tsc",
    "postbuild": "npm run copy-assets",
    "copy-assets": "mkdir -p dist/memory && cp -r memory/cybersecurity-books dist/memory/ 2>/dev/null || true",
    "dev": "ts-node bin/cli.js",
    "start": "node dist/bin/cli.js",
    "start:api": "node dist/api/index.js",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testMatch='**/*.integration.test.ts'",
    "test:e2e": "jest --testMatch='**/*.e2e.test.ts'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md,yml,yaml}\"",
    "validate": "npm run typecheck && npm run lint && npm run test",
    "precommit": "npm run format && npm run validate",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "db:reset": "prisma migrate reset && npm run db:seed",
    "docker:build": "docker build -t project-c0di3:latest .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "deploy": "bash scripts/deploy-production.sh",
    "test-production": "bash scripts/test-production.sh",
    "build-llama": "bash scripts/build-llama-production.sh"
  },
  "keywords": [
    "cybersecurity",
    "ai",
    "security",
    "penetration-testing",
    "red-team",
    "blue-team",
    "natural-language",
    "llm",
    "local-ai",
    "privacy"
  ],
  "author": "Core Agent Team",
  "license": "MIT",
  "dependencies": {
    "@jest/expect-utils": "^29.5.0",
    "@prisma/client": "^5.22.0",
    "@types/node": "^20.8.0",
    "@types/prompts": "^2.0.14",
    "argon2": "^0.40.3",
    "axios": "^1.10.0",
    "chalk": "^5.3.0",
    "clipboardy": "^3.0.0",
    "commander": "^11.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cosmiconfig": "^9.0.0",
    "dotenv": "^16.3.1",
    "execa": "^7.1.1",
    "expect": "^29.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "fast-glob": "^3.3.1",
    "helmet": "^8.0.0",
    "jest-diff": "^29.5.0",
    "jest-matcher-utils": "^29.5.0",
    "jsonwebtoken": "^9.0.2",
    "ora": "^7.0.1",
    "p-map": "^5.5.0",
    "pretty-format": "^29.5.0",
    "prompts": "^2.4.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.22.4",
    "zx": "^7.2.2"
  },
  "devDependencies": {
    "@types/argon2": "^0.15.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.38.0",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "prisma": "^5.22.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Senpai-Sama7/Project-C0Di3.git"
  },
  "bugs": {
    "url": "https://github.com/Senpai-Sama7/Project-C0Di3/issues"
  },
  "homepage": "https://github.com/Senpai-Sama7/Project-C0Di3#readme"
}
