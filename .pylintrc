[MASTER]
ignore=venv
init-hook='import sys; from pathlib import Path; sys.path.append(str(Path(__file__).resolve().parent / "services" / "log-analyzer"))'

[FORMAT]
max-line-length=100

[MESSAGES CONTROL]
disable=
    missing-module-docstring,
    missing-function-docstring

[TYPECHECK]
ignored-modules=
    flask,
    pandas,
    sklearn
